{
    "name": "mrself/mrfile",
    "type": "project",
    "license": "MIT",
    "description": "Symfony package for form tree type",
    "require": {
        "php": "^7.2.0",
        "aws/aws-sdk-php": "^3.112",
        "kunalvarma05/dropbox-php-sdk": "^0.2.1",
        "mrself/container": "^1.7",
        "mrself/options": "^2.14",
        "nicolab/php-ftp-client": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mrself\\File\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrself\\File\\Tests\\": "./tests/"
        }
    }
}
