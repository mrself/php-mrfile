{
    "name": "mrself/mrfile",
    "type": "project",
    "license": "MIT",
    "description": "Symfony package for form tree type",
    "require": {
        "php": "^7.2.0",
        "mrself/options": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mrself\\File\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrself\\File\\Tests\\": "./tests/"
        }
    }
}
